<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ms.teach.by.pojo.Grades" table="grades" catalog="ms">
        <id name="gradeId" type="java.lang.Integer">
            <column name="grade_id" />
            <generator class="identity" />
        </id>
        <property name="gradeNumber" type="java.lang.String">
            <column name="gradeNumber" length="50" not-null="true" />
        </property>
        <property name="gradeName" type="java.lang.String">
            <column name="gradeName" length="50" />
        </property>
    </class>
</hibernate-mapping>
